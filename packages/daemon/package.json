{
  "name": "@agentwatch/daemon",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc",
    "dev": "bun run --watch src/dev.ts",
    "dev:debug": "DEBUG=1 bun run --watch src/dev.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agentwatch/core": "workspace:*",
    "@agentwatch/monitor": "workspace:*",
    "@agentwatch/pre-share": "workspace:*",
    "@agentwatch/transcript-parser": "workspace:*",
    "@huggingface/hub": "^0.18.0",
    "hono": "^4.6.0",
    "node-pty": "^1.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.0.0"
  }
}
